{
    "parser": "babel-eslint",
    "rules": {
        "no-unused-vars": "warn",
        "linebreak-style": 0,
        "react/jsx-filename-extension": [ 1,{ "extensions": [".js", ".jsx"]}]
    },
    "env": {
        "es6": true,
        "browser": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "plugins": ["react"],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        }
    }
}